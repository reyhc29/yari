<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Yari.MySql</id>
    <version>1.0.0</version>
    <authors>Reynaldo Hidalgo</authors>
    <owners>Reynaldo Hidalgo</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/reyhc29/yari</projectUrl>
    <description>Yari for MySql databases. Easily execute mysql stored procedures and functions from the browser; just deploying a single method Web.API service. Also useful to create a Data Access Layer for mysql in any .net core application</description>
    <tags>mysql, spa, json, .net core, dal, middletier, ef alternative</tags>
    <repository type="github" url="https://github.com/reyhc29/yari.git" />
    <dependencies>
      <group targetFramework=".NETCoreApp2.1">
        <dependency id="Newtonsoft.Json" version="11.0.2" exclude="Build,Analyzers"/>
        <dependency id="MySql.Data" version="8.0.12" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <references>
        <group targetFramework=".NETCoreApp2.1">
            <reference file="Yari.dll" />
            <reference file="Yari.MySql.dll" />
        </group>
    </references>
  </metadata>
</package>